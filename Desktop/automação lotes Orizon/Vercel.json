{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/process",
      "dest": "api/process.py"
    },
    {
      "src": "/api/health",
      "dest": "api/health.py"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|ico|svg))",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "index.html"
    }
  ]
}